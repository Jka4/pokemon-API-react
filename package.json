	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"react-app",
			"eslint:recommended"
		],
		"parserOptions": {
			"parser": "babel-eslint"
		},
		"rules": {
			"no-unused-vars": "off"
